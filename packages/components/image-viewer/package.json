{
  "name": "@zeitui-org/image-viewer",
  "version": "0.1.11",
  "description": "Image viewer component with zoom and pan capabilities",
  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src --dts",
    "build:fast": "tsup src",
    "watch": "yalc push --watch",
    "dev": "pnpm build:fast --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "tailwindcss": "^3.0.0"
  },
  "devDependencies": {
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "clean-package": "^2.2.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "dependencies": {
    "@heroui/button": "^2.2.10",
    "@zeitui-org/icons": "workspace:*",
    "@zeitui-org/file-utils": "workspace:*"
  },
  "clean-package": "../../../clean-package.config.json"
}
